{
  "name": "slanting-steppe",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "update": "nx migrate latest",
    "postinstall": "node ./decorate-angular-cli.js",
    "serve:all": "nx run-many --target=serve --projects=frontend-client,frontend-admin,backend-api --parallel=3",
    "build:all": "nx run-many --target=build --projects=frontend-client,frontend-admin,backend-api --parallel=3",
    "serve:frontend-client": "nx run frontend-client:serve",
    "serve:backend-api": "nx run backend-api:serve",
    "typeorm": "typeorm-ts-node-commonjs",
    "backend-api:entities": "node_modules/.bin/tsc --project apps/backend/api/tsconfig.entities.json",
    "backend-api:migrations:run": "node_modules/.bin/tsc --project apps/backend/api/tsconfig.migrations.json && node_modules/.bin/typeorm-ts-node-commonjs migration:run -t=false -d ormconfig.ts",
    "backend-api:migrations:revert": "node_modules/.bin/ts-node node_modules/.bin/typeorm migration:revert",
    "backend-api:migration:create": "cd apps/backend/api/src/migrations/ && typeorm migration:create"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "15.2.2",
    "@angular/cdk": "15.2.2",
    "@angular/common": "15.2.2",
    "@angular/compiler": "15.2.2",
    "@angular/core": "15.2.2",
    "@angular/forms": "15.2.2",
    "@angular/platform-browser": "15.2.2",
    "@angular/platform-browser-dynamic": "15.2.2",
    "@angular/router": "15.2.2",
    "@nestjs/apollo": "^10.1.7",
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/graphql": "^10.1.7",
    "@nestjs/jwt": "^10.0.2",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/sequelize": "^9.0.0",
    "@nestjs/typeorm": "^9.0.1",
    "@nrwl/angular": "15.8.6",
    "@nrwl/nest": "15.8.6",
    "apollo-server-express": "^3.11.1",
    "bcrypt": "^5.1.0",
    "dotenv": "^16.0.3",
    "graphql": "^16.6.0",
    "graphql-tools": "^8.3.14",
    "graphql-type-json": "^0.3.2",
    "hamburgers": "^1.2.1",
    "hammerjs": "^2.0.8",
    "mysql2": "^3.2.0",
    "ng-gallery": "^8.0.1",
    "ngx-simple-modal": "^1.4.17",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
